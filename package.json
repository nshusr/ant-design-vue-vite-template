    "vue-router": "^4.0.0-beta.13"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.3",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
